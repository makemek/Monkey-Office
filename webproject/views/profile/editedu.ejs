<%- include ../header.ejs %>
	
	<form action="/editedu?id=<%= index %>" method="post" >	
		
		<label>Email</label>
		  <input type="text" tabindex="1" placeholder="Enter Your Email Address" name="email" value ="<%= user.local.email %>" readonly><br>
		<label>Level:</label>
		  <input type="text" tabindex="1" placeholder="Enter Your level" name="level" value ="<%= user.education[index].level %>"  required><br>
		<label>Degree:</label>
		  <input type="text" tabindex="1" placeholder="Enter Your Degree" name="degree" value ="<%= user.education[index].degree %>" required><br>
		<label>University:</label>
		  <input type="text" tabindex="1" placeholder="Enter Your University" name="university" value ="<%= user.education[index].university %>" required><br>
		<label>Year:</label>
		  <input type="text" tabindex="1" placeholder="Enter Your Year" name="year" value ="<%= user.education[index].year %>" required><br>
		    
		<footer>
		  <input type="submit" class="btnLogin" value="Edit" tabindex="1">
		</footer>
	</form>


<%- include ../footer.ejs %>